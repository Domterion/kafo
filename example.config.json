{
    "path": "C:/Users/USER/Downloads",
    "make_folders": true,
    "move_existing": true,
    "delay": 2,
    "dirs": [{
        "name": "Documents",
        "exts": ["txt", "pdf", "pptx", "rtf", "ahk"]
    }, {
        "name": "Images",
        "exts": ["png", "svg"]
    }, {
        "name": "Applications",
        "exts": ["exe", "jar", "msi"]
    }, {
        "name": "Video",
        "exts": ["mp4", "webm"]
    }, {
        "name": "Archives",
        "exts": ["zip", "rar"]
    }, {
        "name": "Audio",
        "exts": ["ogg"]
    }]
}